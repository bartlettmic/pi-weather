{
  "name": "pi-weather",
  "version": "1.0.0",
  "description": "Pull data from local weather station",
  "main": "index.js",
  "scripts": {
    "watch": "sudo dhclient wlan0 && sudo iptables -t nat -I PREROUTING -p tcp --dport 80 -j REDIRECT --to-port 3000 && nodemon server.js -e js,pug",
    "start": "node weather.js && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartlettmic/pi-weather.git"
  },
