  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartlettmic/pi-weather/issues"
  },
  "homepage": "https://github.com/bartlettmic/pi-weather#readme",
  "dependencies": {
    "express": "^4.15.4",
    "md5": "^2.2.1",
    "mongodb": "^2.2.31",
    "node-schedule": "^1.2.4",
    "nodemon": "^1.12.0",
    "particle-api-js": "^6.5.0",
    "pug": "^0.1.0",
    "raspicam": "^0.2.13",
    "socket.io": "^2.0.3"
  }
}
